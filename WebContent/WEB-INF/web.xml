<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">


	<servlet>
		<servlet-name>yggl</servlet-name>
		<servlet-class>com.jdgl.servlet.RoomListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>yggl</servlet-name>
		<url-pattern>/yggl</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.jdgl.login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>vipLog</servlet-name>
		<servlet-class>com.jdgl.servlet.VipLogServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>vipLog</servlet-name>
		<url-pattern>/vipLog</url-pattern>
	</servlet-mapping>

</web-app>